        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Substance.instanceBackbone.identifier"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Substance-container-identifier"/>
        <description value="Identifier of the package/container"/>
        <code value="container-identifier"/>
        <type value="token"/>
        <expression value="Substance.instanceBackbone.identifier"/>
        <xpath value="f:Substance/f:instanceBackbone/f:identifier"/>
        <xpathUsage value="normal"/>
