<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="Device-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Device-device-name"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Device.deviceName"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Device-device-name"/>
        <description value="A server defined search that may match any of the string fields in Device.deviceName or Device.type."/>
        <code value="device-name"/>
        <type value="string"/>
        <expression value="Device.deviceName.name | Device.type.coding.display | Device.type.text"/>
        <xpath value="f:Device/f:deviceName"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
